<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".openSummerForaging"
    android:background="@drawable/background">


    <ImageView
        android:id="@+id/grapeCheckBorder"
        android:layout_width="54dp"
        android:layout_height="52dp"
        android:layout_marginStart="144dp"
        android:layout_marginLeft="144dp"
        android:layout_marginTop="210dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="60dp"
        app:layout_constraintBottom_toTopOf="@+id/spiceBerryCheckBorder"
        app:layout_constraintEnd_toStartOf="@+id/borderGrape"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bundle_slot" />

    <ImageView
        android:id="@+id/borderGrape"
        android:layout_width="54dp"
        android:layout_height="52dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="210dp"
        android:layout_marginEnd="144dp"
        android:layout_marginRight="144dp"
        android:layout_marginBottom="60dp"
        app:layout_constraintBottom_toTopOf="@+id/borderSpiceBerry"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/grapeCheckBorder"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bundle_slot" />

    <ImageView
        android:id="@+id/borderSpiceBerry"
        android:layout_width="54dp"
        android:layout_height="52dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="144dp"
        android:layout_marginRight="144dp"
        android:layout_marginBottom="60dp"
        app:layout_constraintBottom_toTopOf="@+id/borderSweetPea"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/spiceBerryCheckBorder"
        app:layout_constraintTop_toBottomOf="@+id/borderGrape"
        app:srcCompat="@drawable/bundle_slot" />

    <ImageView
        android:id="@+id/borderSweetPea"
        android:layout_width="54dp"
        android:layout_height="52dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="144dp"
        android:layout_marginRight="144dp"
        android:layout_marginBottom="353dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sweetPeaCheckBorder"
        app:layout_constraintTop_toBottomOf="@+id/borderSpiceBerry"
        app:srcCompat="@drawable/bundle_slot" />

    <ImageView
        android:id="@+id/imgGrape"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/borderGrape"
        app:layout_constraintEnd_toEndOf="@+id/borderGrape"
        app:layout_constraintStart_toStartOf="@+id/borderGrape"
        app:layout_constraintTop_toTopOf="@+id/borderGrape"
        app:srcCompat="@drawable/grape" />

    <ImageView
        android:id="@+id/imgSpiceBerry"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/borderSpiceBerry"
        app:layout_constraintEnd_toEndOf="@+id/borderSpiceBerry"
        app:layout_constraintStart_toStartOf="@+id/borderSpiceBerry"
        app:layout_constraintTop_toTopOf="@+id/borderSpiceBerry"
        app:srcCompat="@drawable/spice_berry" />

    <ImageView
        android:id="@+id/imgSweetPea"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/borderSweetPea"
        app:layout_constraintEnd_toEndOf="@+id/borderSweetPea"
        app:layout_constraintStart_toStartOf="@+id/borderSweetPea"
        app:layout_constraintTop_toTopOf="@+id/borderSweetPea"
        app:srcCompat="@drawable/sweet_pea" />

    <CheckBox
        android:id="@+id/grape"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/grapeCheckBorder"
        app:layout_constraintEnd_toEndOf="@+id/grapeCheckBorder"
        app:layout_constraintStart_toStartOf="@+id/grapeCheckBorder"
        app:layout_constraintTop_toTopOf="@+id/grapeCheckBorder" />

    <ImageView
        android:id="@+id/spiceBerryCheckBorder"
        android:layout_width="54dp"
        android:layout_height="52dp"
        android:layout_marginStart="144dp"
        android:layout_marginLeft="144dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="60dp"
        app:layout_constraintBottom_toTopOf="@+id/sweetPeaCheckBorder"
        app:layout_constraintEnd_toStartOf="@+id/borderSpiceBerry"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/grapeCheckBorder"
        app:srcCompat="@drawable/bundle_slot" />

    <CheckBox
        android:id="@+id/spiceBerry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/spiceBerryCheckBorder"
        app:layout_constraintEnd_toEndOf="@+id/spiceBerryCheckBorder"
        app:layout_constraintStart_toStartOf="@+id/spiceBerryCheckBorder"
        app:layout_constraintTop_toTopOf="@+id/spiceBerryCheckBorder" />

    <ImageView
        android:id="@+id/sweetPeaCheckBorder"
        android:layout_width="54dp"
        android:layout_height="52dp"
        android:layout_marginStart="144dp"
        android:layout_marginLeft="144dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="353dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/borderSweetPea"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spiceBerryCheckBorder"
        app:srcCompat="@drawable/bundle_slot" />

    <CheckBox
        android:id="@+id/sweetPea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/sweetPeaCheckBorder"
        app:layout_constraintEnd_toEndOf="@+id/sweetPeaCheckBorder"
        app:layout_constraintStart_toStartOf="@+id/sweetPeaCheckBorder"
        app:layout_constraintTop_toTopOf="@+id/sweetPeaCheckBorder" />
</androidx.constraintlayout.widget.ConstraintLayout>